{
  "name": "chevv1/delivery-club-api-sdk",
  "description": "Библиотека для работы с API Delivery Club",
  "type": "library",
  "keywords": ["delivery", "club", "deliveryclub", "api", "sdk"],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "symfony/http-client": "4.4.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^9",
    "symfony/dotenv": "4.4.*",
    "symfony/var-dumper": "4.4.*"
  },
  "autoload": {
    "psr-4": {
      "DeliveryClubSDK\\": "lib/DeliveryClubSDK/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "lib/tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
